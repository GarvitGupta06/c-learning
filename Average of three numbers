#include <stdio.h>
int main (){
float x,y,z;
printf("Enter the first number:");
scanf("%f",&x);
printf("Enter the second number:");
scanf("%f",&y);
printf("Enter the third number:");
scanf("%f",&z);
printf("Average of these numbers is:%f",(x+y+z)/2);
    return 0;
}
